package 쓰레드활용;

public class ThreadMain {

	public static void main(String[] args) {

		String[] key = { "Q", "W", "E" };

		System.out.println("===단일쓰레드===");

		for (int i = 0; i < key.length; i++) {
			Blitzcrank b = new Blitzcrank(key[i]);
			b.run();
		}

		System.out.println("===멀티쓰레드===");
		for (int i = 0; i < key.length; i++) {
			Ari a = new Ari(key[i]);
			
		}
	}

}
