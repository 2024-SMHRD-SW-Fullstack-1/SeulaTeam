package 추상클래스;

public class TempEmployee {

	String empno;
	String name;
	int pay;

	public TempEmployee(String empno, String name, int pay) {
		super();
		this.empno = empno;
		this.name = name;
		this.pay = pay;
	}

	public int getMoneyPay() {
		int result = pay / 12;
		return result;
	}

	public String print() {
		String result = empno + " : " + name + " : " + pay;
		return result;
	}

}
