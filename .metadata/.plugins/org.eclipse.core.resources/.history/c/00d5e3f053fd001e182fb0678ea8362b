package 계산게임;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		PlusGame pg = new PlusGame();
		for (int i = 0; i < 5; i++) {
			pg.makeRandom();
			System.out.print(pg.getQuizMsg());
			int input = sc.nextInt();
			
			pg.checkAnswer(input);
		}
	}

}
